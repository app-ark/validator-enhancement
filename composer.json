{
    "name": "laravelfy/validator-enhancement",
    "description": "Laravel's validator enhancement",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "xiaohui.lam",
            "email": "xiaohui.lam@icloud.com"
        }
    ],
    "require": {
        "laravel/framework": "5.*",
        "php": ">5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "illuminate/container": "5.*",
        "illuminate/http": "5.*",
        "illuminate/routing": "5.*",
        "illuminate/events": "5.*",
        "illuminate/validation": "5.*",
        "filp/whoops": "~1.1.2"
    },
    "autoload": {
        "psr-4": {
            "Laravelfy\\Validator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravelfy\\Validator\\Test\\": "test/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravelfy\\Validator\\ServiceProvider"
            ]
        }
    }
}
